function happy(d)
{
    if(b() == d)
    {
        while(a() < b())
        {
            c = c + 1;
        }
        c = c * sqrt(a() - b());
        happy(c);
    }
    else
    {
        while(a() > b())
        {
            c = c - 1;
        }
        c = c * sqrt(a() + b());
        happy(-c);
    }
}

